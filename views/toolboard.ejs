<%- include('./partials/head') %>
    <%- include('./partials/header') %>

        <main class="toolboard">
            <div class="intro">
                <h2>Check onze partners</h2>
                <p>Dat moet al of moet binnen 2 jaar! Voor overheden is dit al verplicht. En vanaf 2025 moet iedere
                    nieuwe
                    website en app digitaal toegankelijk zijn.
                </p>
                <button>Check een nieuwe website</button>
            </div>
            <dialog class="new-card">

            </dialog>

            <div class="checklist-list">
                <% url_data.urls.forEach(url=> { %>
                    <div class="card">
                        <div>
                            <h4><%= url.website.titel %></h4>

                        </div>
                        <div class="divider">
                            <a href="https://api.vervoerregio-amsterdam.fdnd.nl/api/v1/principes"
                            class="url"><%= url.url %></a>
                        <div class="vortgang">
                            <div>
                                <p>Waneembaar</p>
                                <p>%</p>
                            </div>
                            <div>
                                <p>bedienbaar</p>
                                <p>%</p>
                            </div>
                            <div>
                                <p>begrijpelijk</p>
                                <p>%</p>
                            </div>
                            <div>
                                <p>Robuust</p>
                                <p>%</p>
                            </div>
                        </div>
                        <div class="butten-time">
                            <button class="bekijk-pagina">></button>
                            <div class="time">
                                <p>Aangemaakt op:</p>
                                <p><%= url.createdAt.slice(0, 10) %></p>
                                <button class="card-cancel">X</button>
                            </div>
                        </div>
                        </div>
                        <dialog class="delete-card">

                        </dialog>
                    </div>
                    <%})%>
            </div>
        </main>

        <%- include('./partials/foot') %>